{
    "global": {
        "clean_artifacts": true,
        "out_amount": 1
    },

    "txt2txt": {
        "active": true, 
        "prompt": "any. You can't use these prompts: {global.used_prompts}",
        "prompt_pre": "Generate prompt for AI image generator, make sure it's between \" marks. Subject: {txt2txt.prompt}",
        "save_as_used": true
    },
    "txt2img": {
        "active": true, 
        "prompt": "{txt2txt.out}",
        "height": 768, 
        "width": 768, 
        "matrix": {
            "models": "{txt2img.models}",
            "mix-0": "{global.out_amount}"
        }
    },
    "img2img": {
        "active": false, 
        "prompt": "",
        "image": "",
        "height": 768, 
        "width": 768, 
        "matrix": {
            "models": "{img2img.models}", 
            "mix-0": "{global.out_amount}", 
            "mix-1" : "{txt2img.models}"
        }
    },
    "img_upscale": {
        "active": true, 
        "image": "{txt2img.out}",
        "scale": 3,
        "matrix": {
            "models": "{img_upscale.models}", 
            "mix-0": "{global.out_amount}", 
            "mix-1" : "{txt2img.models}",
            "mix-2" : "{img2img.models}"
        }
    },
    "img2vid": {
        "active": false, 
        "prompt": "",
        "image": "",
        "matrix": {
            "models": "{img2vid.models}", 
            "mix-0": "{global.out_amount}", 
            "mix-1" : "{txt2img.models}",
            "mix-2" : "{img2img.models}"
        }
    }
}
