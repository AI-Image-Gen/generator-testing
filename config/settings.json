{
    "global": {
        "clean_artifacts": true,
        "out_amount": 1
    },

    "txt2txt": {
        "active": true, 
        "prompt": ""
    },
    "txt2img": {
        "active": true, 
        "prompt": "{txt2txt.out}",
        "height": 512, 
        "width": 512,
        "matrix": {
            "models": "{txt2img.models}"
        }
    },
    "txt2vid": {
        "active": true, 
        "prompt": "{txt2txt.out}",
        "video": {
            "enable": true,
            "music": true
        },
        "gif": {
            "enable": true,
            "speed": 100
        },
        "matrix": {
            "models": "{txt2vid.models}"
        }
    },
    "img2img": {
        "active": true, 
        "prompt": "{txt2txt.out}",
        "image": "{txt2img.out}",
        "height": 576, 
        "width": 1024, 
        "matrix": {
            "models": "{img2img.models}"
        }
    },
    "img2vid": {
        "active": true, 
        "image": "{img2img.out}",
        "video": {
            "enable": true,
            "music": true
        },
        "gif": {
            "enable": true,
            "speed": 100
        },
        "matrix": {
            "models": "{img2vid.models}"
        }
    },
    "upscale": {
        "active": true, 
        "input": "{txt2img.out}",
        "scale": 3,
        "matrix": {
            "models": "{upscale.models}"
        }
    }
}
