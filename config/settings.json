{
    "global": {
        "clean_artifacts": true,
        "out_amount": 1
    },

    "txt2txt": {
        "active": true, 
        "prompt": "some nice image",
        "save_as_used": true
    },
    "txt2img": {
        "active": true, 
        "prompt": "{txt2txt.out}",
        "height": 768, 
        "width": 768,
        "matrix": {
            "models": "{txt2img.models}"
        }
    },
    "img2img": {
        "active": false, 
        "prompt": "",
        "image": "",
        "height": 768, 
        "width": 768, 
        "matrix": {
            "models": "{img2img.models}"
        }
    },
    "img_upscale": {
        "active": true, 
        "image": "{txt2img.out}",
        "scale": 3,
        "matrix": {
            "models": "{img_upscale.models}"
        }
    },
    "img2vid": {
        "active": false, 
        "prompt": "",
        "image": "",
        "matrix": {
            "models": "{img2vid.models}"
        }
    }
}
